<div{{ attributes }}>
	<!-- Main navigation container -->
	<nav class=" fixed top-0 flex-no-wrap z-[1050] w-full items-center justify-between bg-[#B0BEC5] py-2 shadow-md
		shadow-black/5 dark:shadow-black/10 lg:flex-wrap lg:justify-start lg:py-4"
			data-te-navbar-ref
	>
		<div class="flex w-full flex-wrap items-center justify-between px-3">
			<!-- Hamburger button for mobile view -->
			<button
					class="block border-0 bg-transparent px-2 text-neutral-500 hover:no-underline hover:shadow-none
					focus:no-underline focus:shadow-none focus:outline-none focus:ring-0 dark:text-neutral-200 lg:hidden"
					type="button"
					data-te-collapse-init
					data-te-target="#navbarSupportedContent12"
					aria-controls="navbarSupportedContent12"
					aria-expanded="false"
					aria-label="Toggle navigation"
			>
				<!-- Hamburger icon -->
				<span class="[&>svg]:w-7">
			        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="h-7 w-7 text-black-50">
				          <path fill-rule="evenodd"
						        d="M3 6.75A.75.75 0 013.75 6h16.5a.75.75 0 010 1.5H3.75A.75.75 0 013 6.75zM3 12a.75.75 0
						        01.75-.75h16.5a.75.75 0 010 1.5H3.75A.75.75 0 013 12zm0 5.25a.75.75 0 01.75-.75h16.5a.75.75
						        0 010 1.5H3.75a.75.75 0 01-.75-.75z"
						        clip-rule="evenodd"
				          />
			        </svg>
                 </span>
			</button>

			<!-- Collapsible navigation container -->
			<div class="!visible hidden flex-grow basis-[100%] items-center lg:!flex lg:basis-auto"
				 id="navbarSupportedContent12"
				 data-te-collapse-item
			>
				<!-- Logo -->
				<a class="mb-4 ml-2 mr-5 mt-3 flex items-center text-neutral-900 hover:text-neutral-900
						 focus:text-neutral-900 dark:text-neutral-200 dark:hover:text-neutral-400
						 dark:focus:text-neutral-400 lg:mb-0 lg:mt-0"
						href="{{ path('app_home') }}"
				>
					<img src="{{ asset('/images/logo-color.png') }}"
						 class="h-8 w-auto sm:h-10"
						 alt="logo"
						 loading="lazy"
					/>
				</a>
				<!-- Left navigation links -->
				<ul class="list-style-none mr-auto flex flex-col pl-0 lg:flex-row" data-te-navbar-nav-ref>

					<!-- MegaMenu -->
					<li class="mb-4 lg:mb-0 lg:pr-2" data-te-nav-item-ref data-te-dropdown-ref>
						<a class=" transition duration-200 hover:text-neutral-700 hover:ease-in-out
								  focus:text-neutral-700 disabled:text-black/30 motion-reduce:transition-none
								 text-[#001F3FFF] dark:hover:text-neutral-300 dark:focus:text-neutral-300
								  lg:px-2 [&.active]:text-black/90 dark:[&.active]:text-neutral-400"
						   href="#"
						   data-te-ripple-init
						   data-te-ripple-color="light"
						   type="button"
						   id="dropdownMenuButtonX"
						   data-te-dropdown-toggle-ref
						   aria-expanded="false"
						   data-te-nav-link-ref
						>
							MENU
						</a>
						<div class="absolute left-0 right-0 top-full z-[1000] mt-0 hidden w-full border-none bg-white
						            bg-clip-padding  shadow-lg bg-[#001F3FFF]
						            [&[data-te-dropdown-show]]:block"
						     aria-labelledby="dropdownMenuButtonX"
						     data-te-dropdown-menu-ref>
							<div class="px-6 py-5 lg:px-8">
								<div class="grid gap-6 md:grid-cols-2 lg:grid-cols-4">
									<!-- MegaMenu categories -->
									<div>
										<h3>Nos catégories</h3>
										{% for category in this.categories | slice (0,10) %}
											<a
													href="{{ path('app_category_show', {'id' : category.id}) }}"
													aria-current="true"
													class="block w-full border-b border-neutral-200 px-6 py-2 transition
													       duration-150 ease-in-out hover:bg-neutral-50 hover:text-neutral-700
													       dark:border-neutral-500 dark:hover:bg-neutral-800 dark:hover:text-white"
											>
												{{ category.name }}
											</a>
										{% endfor %}
									</div>
									<!--/ MegaMenu categories -->

									<!-- MegaMenu authors -->
									<div>
										<h3>Nos auteurs</h3>
										{% for author in this.authors | slice (0,10) %}
											<a href="{{ path('app_author_show', {'id' : author.id}) }}"
											   aria-current="true"
											   class="block w-full border-b border-neutral-200 px-6 py-2 transition
											          duration-150 ease-in-out hover:bg-neutral-50
											          hover:text-neutral-700 dark:border-neutral-500
											          dark:hover:bg-neutral-800 dark:hover:text-white"
											>
												{{ author.name }}
											</a>
										{% endfor %}
									</div>
